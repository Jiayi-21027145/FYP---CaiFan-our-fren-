

@{
    @model List<Menu>

}

@{
    Layout = "_CaiFan";
}

@{
    ViewBag.Title = "Menu";
}


<style>
    body {
        background: url('@(Url.Content("~/BG 3.jpg"))') no-repeat center center fixed;
        background-size: cover;
        font-size: small;
    }
</style>


    <script src="https://cdn.rawgit.com/davidshimjs/qrcodejs/gh-pages/qrcode.min.js"></script>
    <style>
        .text-left p{
            text-align:left;
            display: block
        }
        .text-left img{
            max-width: 15%;
            height:auto;
        }
        .instagram-text {

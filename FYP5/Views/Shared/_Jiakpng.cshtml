<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <script src="~/lib/bootstrap/umd/popper.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.min.js"></script>
    <link href="~/lib/bootstrap/font/bootstrap-icons.min.css" rel="stylesheet" />
    <title>WhereWeWere</title>
</head>
<body>
    @{
        Layout = "_CaiFan";
    }

    @if (User.Identity!.IsAuthenticated)
    {
        if (User.IsInRole("Admin"))
        {
            // Admin users
            <partial name="JiakAdmin" />
        }
        else
        {
            // Logged-in users
            <partial name="JiakUser" />
        }
    }
   @*  else *@
    @*For unauthenticated (guest) users*@
    @* {
        <partial name="TravelGuestNav" />
    }
 *@
    <div class="container mt-10">
        @RenderBody()
    </div>

</body>
</html>

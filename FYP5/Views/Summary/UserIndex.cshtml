@{
    Layout = "_CaiFan";
}



<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <link href="css/userSummary.css" rel="stylesheet" />
    <link href="~/css/profile.css" rel="stylesheet" />

    <div class="row">
        <h2>User Summary</h2>
        <ul class="nav nav-tabs fw-bold">
            <li class="nav-item"><a class="nav-link active" data-bs-toggle="tab" data-bs-target="#Summary1">Calorie Count for the past Month</a></li>
            <li class="nav-item"><a class="nav-link" data-bs-toggle="tab" data-bs-target="#Summary2">Amount Spent vs Average Amount</a></li>
        </ul>

        <div class="tab-content" style="margin-top:10px">
            <div id="Summary1" class="tab-pane fade show active">
                <div>
                    @{
                        <img src="/images/user01_cc.jpg" />
                    }
                </div>
            </div>


            <div id="Summary2" class="tab-pane fade">
                @{
                    <img src="/images/avg_price.jpg" />

                }
            </div>

        </div>


    </div>
</head>
</html>